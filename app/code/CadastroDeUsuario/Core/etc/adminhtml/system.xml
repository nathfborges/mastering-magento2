<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
    <system>
        <tab id="cadastro" translate="label" sortOrder="10000">
            <label>Cadastro</label>
        </tab>
        <section id="cadastros" translate="label" sortOrder="10" showInDefault="1" showInWebsite="0" showInStore="0">
            <label>Log</label>
            <tab>cadastro</tab>
            <resource>CadastroDeUsuario_Core::cadastros</resource>

            <group id="log" translate="label" sortOrder="10" showInDefault="1" showInWebsite="0" showInStore="0">
